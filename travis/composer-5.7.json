  "require-dev": {
    "orchestra/testbench": "3.7.*",
    "laravel/framework": "5.7.*",
    "phpunit/phpunit": "^7.4",
    "phpunit/php-code-coverage": "^6.0",
    "codacy/coverage": "^1.0"
  }
}
