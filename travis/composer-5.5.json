{
  "name": "marcin-orlowski/laravel-api-response-builder",
  "require": {
    "php": ">=7.0"
  },
  "autoload": {
    "psr-4": {
      "MarcinOrlowski\\ResponseBuilder\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MarcinOrlowski\\ResponseBuilder\\": "src",
      "MarcinOrlowski\\ResponseBuilder\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "orchestra/testbench": "3.5.*",
    "laravel/framework": "5.5.*",
    "phpunit/phpunit": "^6.0",
    "phpunit/php-code-coverage": "^5.3",
    "codacy/coverage": "^1.0"
  }
}
